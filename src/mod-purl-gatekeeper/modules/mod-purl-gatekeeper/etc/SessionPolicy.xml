<?xml version="1.0" encoding="UTF-8"?>
<SessionPolicy>
	<zone>
		<match>.*admin.*</match>
		<path>/</path>
		<token>cookie</token>
		<type>
			<simple/>
		</type>
	</zone>
	<meta>
		<id>1060NetKernelSession</id>
		<max-sessions>3000</max-sessions>
		<max-duration>14400000</max-duration>  <!--2 hours-->
		<min-duration>0</min-duration>  <!--Oldest drop when max-sessions is reached-->
	</meta>
</SessionPolicy>
