<?xml version="1.0" encoding="ISO-8859-1"?>
<purls>
<!-- Type 301.  Moved Permanently to a target URL -->
	<purl id="/testdomain/test301"  type="301"/>
<!-- Type 302.  Simple redirection to a target URL -->
	<purl id="/testdomain/test302"  type="302"/>
<!-- Type 303.  See Also URL -->
	<purl id="/testdomain/test303/"  type="303"/>
<!-- Type 307.  Temporary redirect to a target URL -->
	<purl id="/testdomain/test307"  type="307"/>
<!-- Type 404.  Temporarily gone -->
	<purl id="/testdomain/test404"  type="404"/>
<!-- Type 410.  Permanently gone -->
	<purl id="/testdomain/test410"  type="410"/>
</purls>
