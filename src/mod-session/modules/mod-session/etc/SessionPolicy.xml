<SessionPolicy>
	<zone>
		<match>.*/(template|purl|group|domain|user|admin)/.*</match>
		<path>/(template|purl|group|domain|user|admin)</path>
		<token>cookie</token>
		<type>
			<simple />
		</type>
	</zone>
	<meta>
		<id>1060NetKernelSession</id>
		<max-sessions>500</max-sessions>
		<max-duration>7200000</max-duration>  <!--2 hours-->
		<min-duration>0</min-duration>  <!--Oldest drop when max-sessions is reached-->
	</meta>
</SessionPolicy>